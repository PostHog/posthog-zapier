{
  "name": "posthog-zapier",
  "version": "1.0.0",
  "description": "Zapier integration for PostHog.",
  "license": "MIT",
  "main": "index.js",
  "scripts": {
    "clean": "rimraf ./lib ./build",
    "build": "npm run clean && tsc",
    "watch": "npm run clean && tsc --watch",
    "test": "npm run build && jest --testTimeout 10000 --rootDir ./lib/test",
    "push": "npm run build && zapier test && zapier push"
  },
  "dependencies": {
    "zapier-platform-core": "10.0.1"
  },
  "devDependencies": {
    "@types/jest": "^25.2.1",
    "@types/node": "^13.13.5",
    "jest": "^25.5.3",
    "prettier": "^2.0.5",
    "rimraf": "^3.0.2",
    "typescript": "^3.9.7"
  },
  "private": true
}
